{
    "name": "soitpl/laravel-modules",
    "type": "library",
    "description": "Library for manage modules in Laravel",
    "homepage": "https://github.com/soitpl/laravel-modules",
    "license": "MIT",
    "version": "0.1.2",
    "keywords": [
        "modules",
        "laravel"
    ],
    "authors": [
        {
            "name": "Rafal Tadaszak",
            "email": "r.tadaszak@soit.pl",
            "homepage": "http://soit.pl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/dbal": "^2.9",
        "laravel/framework": "^8 || ^9",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6 || ^7",
        "mikey179/vfsstream": "^2.0@dev",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.11.15",
        "fakerphp/faker": "^1.9"
    },
	"autoload": {
        "psr-4": { "soIT\\LaravelModules\\": "src/",  "soIT\\LaravelModules\\Tests\\Unit\\": "tests/Unit/"}
    }
}
